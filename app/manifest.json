{
    "name": "chrome-extension-with-media",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Chrome Extension using Media",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "images/icon-16.png",
        "default_title": "Sayuri Client",
        "default_popup": "popup.html"
    },
    "permissions": ["notifications"]
}
